{
  "schemaVersion": 1,
  "id": "warpsigns",
  "version": "${version}",

  "name": "WarpSigns",
  "description": "A simple mod that lets you pair sets of signs together to allow teleporting between them.",
  "authors": [
    "Alex Meyer"
  ],
  "contact": {
    "homepage": "https://github.com/reyemxela/warpsigns",
    "sources": "https://github.com/reyemxela/warpsigns"
  },

  "license": "CC0-1.0",
  "icon": "assets/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.reyemxela.warpsigns.WarpSigns"
    ]
  },

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },

  "mixins": [
    "warpsigns.mixins.json"
  ]
}
